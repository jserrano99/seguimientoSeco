	<parameter name="certificadoServiciosId" class="java.lang.Integer">
		<parameterDescription><![CDATA[Certificado de Servicios]]></parameterDescription>
	</parameter>
	<queryString language="SQL">
		<![CDATA[select t1.descripcion as certificadoDs,
t1.contador_NPL_CRI as totalNPL ,
t1.penalizados_CRI as penalizados, 
t4.descripcion as objetoDs
from certificado_servicios as t1 
inner join encargo_penalizado as t2 on t2.certificado_servicios_id = t1.id 
inner join encargo as t3 on t3.id = t2.encargo_id 
inner join objetos_encargo as t4 on t4.id = t3.objeto_encargo_id 
where t1.id = $P{certificadoServiciosId}
and t2.indicador_id = 1]]>
	</queryString>
