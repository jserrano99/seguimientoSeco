	<parameter name="certificadoServiciosId" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select t1.descripcion as contratoDs,
t2.descripcion as certificadoServiciosDs,
concat (t3.descripcion,' ,', t4.descripcion) as periodo
 from contrato as t1 
inner join  certificado_servicios as t2 on t2.contrato_id =  t1.id 
inner join meses as t3 on t3.id = t2.meses_id  
inner join anyos as t4 on t4.id = t3.anyo_id
where t2.id = $P{certificadoServiciosId}]]>
	</queryString>
