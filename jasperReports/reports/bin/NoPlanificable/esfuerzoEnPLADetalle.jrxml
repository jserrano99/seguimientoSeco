	<queryString language="SQL">
		<![CDATA[SELECT  objetos_encargo.codigo, objetos_encargo.descripcion,
encargo.numero , encargo.titulo, encargo.horas_realizadas,
t1.certificado_servicios_id as certificadoID,
encargo.fc_entrega,
encargo.fc_aceptacion
 FROM ENCARGO 
INNER JOIN OBJETOS_ENCARGO ON OBJETOS_ENCARGO.ID = encargo.objeto_encargo_id
LEFT JOIN linea_certificado as t1 on t1.encargo_id = encargo.id
WHERE objetos_encargo.tipo_objeto_id = 4  and 
encargo.estado_actual_id  in ('2','12' ) and
encargo.fc_entrega between str_to_date($P{FC_INICIO},'%d/%m/%Y %H:%i:%s')  and str_to_date($P{FC_FIN},'%d/%m/%Y  %H:%i:%s') 
order by objetos_encargo.descripcion, encargo.numero]]>
	</queryString>
