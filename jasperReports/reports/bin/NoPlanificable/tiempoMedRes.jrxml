	<queryString language="SQL">
		<![CDATA[SELECT objetos_encargo.codigo, 
objetos_encargo.descripcion,
encargo.numero, 
encargo.tiempo_resolucion
FROM ENCARGO 
INNER JOIN OBJETOS_ENCARGO ON OBJETOS_ENCARGO.ID = encargo.objeto_encargo_id
inner join aplicacion on aplicacion.id = encargo.aplicacion_id
WHERE objetos_encargo.tipo_objeto_id = 1 
AND encargo.fc_entrega BETWEEN 
str_to_date($P{FC_INICIO},'%d/%m/%Y')  and str_to_date($P{FC_FIN},'%d/%m/%Y') 
and encargo.objeto_encargo_id  <> 5
order by objetos_encargo.codigo]]>
	</queryString>
