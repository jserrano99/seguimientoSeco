	<parameter name="certificadoId" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select t4.descripcion as agrupacionDs, 
t3.descripcion as objetoEncargoDs, t2.numero , t2.titulo,
t2.horas_comprometidas, t2.coste, t4.codigo as agrupacionCD
 from linea_certificado as t1
inner join encargo as t2 on t2.id = t1.encargo_id
inner join agrupacion as t4 on t4.id = t2.agrupacion_id
inner join objetos_encargo as t3 on t3.id = t2.objeto_encargo_id 
where t1.certificado_servicios_id = $P{certificadoId}
and t1.tipo_cuota_id = 2 
and t4.tipo_agrupacion_id = 1
order by agrupacionDs]]>
	</queryString>
