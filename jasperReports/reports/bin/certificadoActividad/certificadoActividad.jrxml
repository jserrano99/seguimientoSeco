	<queryString>
		<![CDATA[select contrato.id as contratoId,
contrato.codigo as contratoCodigo,
contrato.descripcion as contratoDescripcion,
contrato.fc_inicio as contratoFechaInicio,
contrato.fc_fin as contratoFechaFin,
contrato.centro_coste_cd as contratoCentroCosteCd,
contrato.centro_coste_ds as contratoCentroCosteDs,
contrato.expediente as contratoExpediente,
contrato.adjudicatario as contratoAdjudicatario,
contrato.importe_contrato as contratoImporteContrato,
contrato.importe_adjudicacion as contratoImporteAdjudicacion,
contrato.porcentaje_baja as contratoPorcentajeBaja,
contrato.numero_pedido as contratoNumeroPedido,
certificado_servicios.id as certificadoServiciosId,
certificado_servicios.contrato_id as certificadoServiciosContratoId,
certificado_servicios.descripcion as certificadoSserviciosDescripcion,
importes_contrato.fc_inicio as importesContratoFechaInicio,
importes_contrato.fc_fin as importesContratoGechaFin,
importes_contrato.cuota_fija as importesContratoCuotaFija,
importes_contrato.cuota_tasada as importesContratoCuotaTasada,
importes_contrato.tarifa_hora as importesContratoTarifaHora,
importes_contrato.cuota_fija_mensual as importesContratoCuotaFijaMensual,
meses.descripcion as mesesDescripcion, 
anyos.descripcion as anyosDescripcion,
concat(meses.descripcion,', ',anyos.descripcion) as periodo,
certificado_servicios.total_factura  as totalFactura,
certificado_servicios.total_penalizaciones as totalPenalizaciones,
certificado_servicios.maximo_penalizaciones as maximoPenalizaciones,
certificado_servicios.cuota_iva as cuotaIva,
certificado_servicios.total_factura_con_iva as totalFacturaConIva,
certificado_servicios.penalizacion_aplicable as penalizacionAplicable
from certificado_servicios 
inner join contrato on contrato.id = certificado_servicios.contrato_id 
inner join importes_contrato on importes_contrato.contrato_id = contrato.id
inner join meses on meses.id = certificado_servicios.meses_id
inner join anyos on anyos.id = meses.anyo_id
where  certificado_servicios.id = $P{certificadoServiciosId}]]>
	</queryString>
