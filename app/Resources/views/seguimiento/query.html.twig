{% extends "base.html.twig" %}
{% block title %} LINEAS DE SEGUIMIENTO {% endblock %}
{% block titulo %} 
    <div id='dtitulo' class="container"> LINEA DE SEGUIMIENTO **CONSULTA**
    <a href="{{path("addSeguimiento")}}"
       class="btn btn-success">
        <span class="glyphicon glyphicon-new-window"></span> Nueva linea de Seguimiento</a>
    <hr/>
    </div>
{% endblock %} 

{% block contenido %}
    <div class="container-fluid">
        {% for message in app.session.flashbag().get('status') %}
            <div class="alert alert-success">
                {{message}}
            </div>
            <hr/>
        {% endfor %}
        {{ sg_datatables_render(datatable) }}
    </div>
{% endblock %}
